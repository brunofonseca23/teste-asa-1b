;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	asa.rn.br. root.asa.rn.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  NS  mail.asa.rn.br.
@	IN	NS	ns1.asa.rn.br.

161 IN PTR	mail.asa.rn.br.
161	IN PTR 	ns1.asa.rn.br.

